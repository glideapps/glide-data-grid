{
  "name": "@workfront-grid/glide-data-grid-cells",
  "version": "5.14.0",
  "description": "Extra cells for glide-data-grid",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "browser": "dist/js/index.js",
  "main": "dist/cjs/index.js",
  "module": "dist/js/index.js",
  "types": "dist/ts/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/js/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./dist/index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build-js && npm run build-types",
    "build-js": "node build.cjs && tsc-esm-fix --target='dist/js'",
    "build-types": "tsc -p tsconfig.types.json",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/workfront/glide-data-grid.git",
    "directory": "packages/cells"
  },
  "homepage": "https://github.com/workfront/glide-data-grid/tree/main/cells",
  "author": "Glide",
  "license": "MIT",
  "keywords": [
    "react",
    "datagrid",
    "data-grid",
    "editor",
    "reactjs",
    "scrolling",
    "data",
    "table",
    "cell",
    "canvas"
  ],
  "dependencies": {
    "@toast-ui/editor": "3.1.10",
    "@toast-ui/react-editor": "3.1.10",
    "@workfront-grid/glide-data-grid": "latest",
    "react-select": "^5.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@types/prosemirror-commands": "^1.0.4",
    "@types/react": "16.14.21",
    "eslint": "8.43.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "react-resize-detector": "^7.1.2",
    "tsc-esm-fix": "^2.7.8",
    "typescript": "^4.5.2"
  }
}
