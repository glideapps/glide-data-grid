        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-sonarjs": "^0.13.0",
        "eslint-plugin-unicorn": "^43.0.2",
        "http-server": "^14.1.1",
        "jsdom": "^22.1.0",
